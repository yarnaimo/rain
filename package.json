{
  "name": "@yarnaimo/rain",
  "version": "0.2.8",
  "repository": "github:yarnaimo/rain",
  "license": "MIT",
  "author": "yamaimo <yarnaimo@gmail.com>",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "debug": "node --inspect -r ts-node/register",
    "dev": "yarn ts-node src/index.ts",
    "prepare": "yarn build",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "NODE_ENV=test jest",
    "ts-node": "ts-node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,vue,gql,md}": [
      "prettier --write --ignore-path .gitignore",
      "git add"
    ],
    "./package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "dependencies": {
    "@types/got": "^9.4.4",
    "fp-ts": "^1.17.1",
    "got": "^9.6.0",
    "io-ts": "^1.8.5",
    "io-ts-types": "^0.4.7",
    "monocle-ts": "^1.7.1",
    "newtype-ts": "^0.2.4"
  },
  "devDependencies": {
    "@yarnaimo/tss": "^0.1.5"
  }
}
